{
    "version": 2,
    "builds": [
      {
        "src": "dashboard.html",
        "use": "@vercel/python",
        "config": {
          "zeroConfig": true
        }
      }
    ],
    "routes": [
      { "src": "/login(.*)", "dest": "dashboard.html" }
    ],
    "env": {
      "FLASK_ENV": "production",
      "SECRET_KEY": "your_secret_key"
    }
  }
  